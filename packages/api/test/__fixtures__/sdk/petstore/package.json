{
  "name": "@api/petstore",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "@readme/api-core": "latest",
    "json-schema-to-ts": "latest",
    "oas": "latest"
  },
  "tsup": {
    "cjsInterop": true,
    "clean": true,
    "dts": true,
    "entry": [
      "./src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "minify": false,
    "shims": true,
    "sourcemap": true,
    "splitting": true
  }
}
